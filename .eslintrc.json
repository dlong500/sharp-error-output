{
  "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "warn",
    "no-unexpected-multiline": "error",
    "no-unused-vars": 1,
    "comma-dangle": [1, "always-multiline"]
  }
}